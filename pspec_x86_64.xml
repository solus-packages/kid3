            <Path fileType="doc">/usr/share/doc</Path>
            <Path fileType="data">/usr/share/icons</Path>
            <Path fileType="man">/usr/share/man/de/man1/kid3-qt.1.gz</Path>
            <Path fileType="man">/usr/share/man/man1/kid3-qt.1.gz</Path>
